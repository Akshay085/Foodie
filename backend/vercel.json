{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/**/*",
        "use": "@vercel/static"
      },
      {
        "api/*": {"runtime": "nodejs18.x"}
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ]
}